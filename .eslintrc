{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": ["jest"],  
  "parserOptions": {
    "ecmaVersion": 2020,
     "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}